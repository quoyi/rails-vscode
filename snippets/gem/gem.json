{
  "gem-development": {
    "prefix": "gem-development",
    "body": [
      "group :development do",
      "\tgem '${1:name}'${2:, 'version'}",
      "end"
    ],
    "description": "Develpment Gem"
  },
  "gem-test": {
    "prefix": "gem-test",
    "body": [
      "group :test do",
      "\tgem '${1:name}''${2:, 'version'}",
      "end"
    ],
    "description": "Test Gem"
  },
  "gem-production": {
    "prefix": "gem-production",
    "body": [
      "group :production do",
      "\tgem '${1:name}''${2:, 'version'}",
      "end"
    ],
    "description": "Production Gem"
  },
  "gem-development-test": {
    "prefix": "gem-development-test",
    "body": [
      "group :development, :test do",
      "\tgem '${1:name}''${2:, 'version'}",
      "end"
    ],
    "description": "Develpment & Test Gem"
  },
  "gem": {
    "prefix": "gem",
    "body": "gem '${1:name}'${2, 'version'}",
    "description": "Add gem"
  }
}
