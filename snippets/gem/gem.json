{
  "gem-development": {
    "prefix": "gem-development",
    "body": "group :${4:development},${2: do}\n\tgem '${1:name}' 'version'\n\t$3\nend\n",
    "description": "Develpment Gem"
  },
  "gem-test": {
    "prefix": "gem-test",
    "body": "group :${4:test},${2: do}\n\tgem '${1:name}' 'version'\n\t$3\nend\n",
    "description": "Test Gem"
  },
  "gem-production": {
    "prefix": "gem-production",
    "body": "group :${4:production},${2: do}\n\tgem '${1:name}' 'version'\n\t$3\nend\n",
    "description": "Production Gem"
  },
  "gem-development-test": {
    "prefix": "gem-development-test",
    "body": "group :${4::development},${2: test do}\n\tgem '${1:name}' 'version'\n\t$3\nend\n",
    "description": "Develpment & Test Gem"
  }
}
